{
  "log": {
    "loglevel": "warning",
    "access": "none",
    "dnsLog": false
  },
  "stats": {},
  "api": {
    "tag": "api",
    "services": ["HandlerService", "StatsService"]
  },
  "dns": {
    "disableCache": true,
    "servers": ["bind"]
  },
  "policy": {
    "levels": {
      "0": {
        "connIdle": 300,
        "bufferSize": 512,
        "statsUserUplink": true,
        "statsUserDownlink": true
      }
    }
  },
  "routing": {
    "rules": [
      {
        "type": "field",
        "inboundTag": ["api"],
        "outboundTag": "api"
      },
      {
        "type": "field",
        "port": 53,
        "network": "tcp,udp",
        "outboundTag": "bind"
      }
    ]
  },
  "inbounds": [
    {
      "tag": "api",
      "protocol": "dokodemo-door",
      "listen": "/tmp/xray/api.sock,766",
      "settings": {
        "address": "",
        "network": "unix"
      }
    },
    {
      "tag": "vless-tcp",
      "protocol": "vless",
      "listen": "/tmp/xray/xray.sock,766",
      "settings": {
        "decryption": "none",
        "clients": [],
        "fallbacks": [
          {
            "dest": "@ws",
            "path": "/ws"
          },
          {
            "dest": "/tmp/nginx/fallback.sock"
          }
        ]
      },
      "streamSettings": {
        "network": "tcp",
        "security": "tls",
        "tlsSettings": {
          "rejectUnknownSni": false,
          "minVersion": "1.3",
          "cipherSuites": "TLS_AES_128_GCM_SHA256:TLS_AES_256_GCM_SHA384:TLS_CHACHA20_POLY1305_SHA256",
          "certificates": [
            {
              "ocspStapling": 3600,
              "certificateFile": "/etc/letsencrypt/live/$DOMAIN/fullchain.pem",
              "keyFile": "/etc/letsencrypt/live/$DOMAIN/privkey.pem"
            }
          ]
        },
        "sockopt": {
          "tcpFastOpen": true
        }
      }
    },
    {
      "tag": "vless-ws",
      "protocol": "vless",
      "listen": "@ws",
      "settings": {
        "decryption": "none",
        "clients": []
      },
      "streamSettings": {
        "network": "ws",
        "security": "none",
        "wsSettings": {
          "path": "/ws"
        }
      }
    }
  ],
  "outbounds": [
    {
      "tag": "freedom",
      "protocol": "freedom",
      "domainStrategy": "UseIP"
    },
    {
      "tag": "blackhole",
      "protocol": "blackhole"
    },
    {
      "tag": "bind",
      "protocol": "dns",
      "settings": {
        "address": "bind"
      }
    }
  ]
}
